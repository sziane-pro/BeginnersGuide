{
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/Scripts/python.exe",
    "robot.language-server.python": "${workspaceFolder}/.venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "robot.completions.keywords.prefixImportName": true,
    "robot.completions.keywords.prefixImportNameIgnore": ["BuiltIn"],
    "robot.python.env": {
        "PROJECT_FOLDER": "${workspaceFolder}"
    },
    "editor.renderWhitespace": "all",
}